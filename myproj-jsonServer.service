[Unit]
Description=myproj-jsonServer
After=network.target
After=syslog.target

[Service]
Environment="NODE_ENV=test"
Type=forking
ExecStart=/usr/local/bin/forever -a start -l /appServer/myproj/jsonServer/forever.log /appServer/myproj/jsonServer/start.js
ExecReload=/usr/local/bin/forever restart /appServer/myproj/jsonServer/start.js
ExecStop=/usr/local/bin/forever stop /appServer/myproj/jsonServer/start.js
PrivateTmp=true

[Install]
WantedBy=multi-user.target